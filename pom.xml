				<spring.rabbitmq.username>wsh</spring.rabbitmq.username>
				<spring.rabbitmq.password>wsh12345678</spring.rabbitmq.password>
				<eureka.client.serviceUrl.defaultZone>
					http://172.18.182.192:7000/eureka/,

				<property>
					<name>environment.type</name>
					<value>env-dev</value>
				</property>
			</activation>
			<properties>
				<profileActive>dev</profileActive>
				<log_root>C:/Users/Administrator/Desktop/logs</log_root>
				<console_log_charset>UTF-8</console_log_charset>
				<logger_package>cn.com.inlee</logger_package>
				<spring.rabbitmq.host>125.69.76.147</spring.rabbitmq.host>
				<spring.rabbitmq.port>5672</spring.rabbitmq.port>
				<spring.rabbitmq.username>rmq_zipkin</spring.rabbitmq.username>
				<spring.rabbitmq.password>123456</spring.rabbitmq.password>
				<eureka.client.serviceUrl.defaultZone>http://125.69.76.147:7000/eureka/,http://125.69.76.147:7001/eureka/</eureka.client.serviceUrl.defaultZone>
			</properties>
		</profile>

		<profile>
			<id>env-test</id>
			<activation>
				<property>
